/* 滚动条 */
::-webkit-scrollbar {
  width:9px;
  height: 9px;
}
/* 滚动槽 */
::-webkit-scrollbar-track {
  -webkit-box-shadow: #0F84FD;
  border-radius:10px;
}
/* 滚动条滑块 */
::-webkit-scrollbar-thumb {
  border-radius:10px;
  background-color:#0F84FD;
  -webkit-box-shadow:#0F84FD;
}
::-webkit-scrollbar-thumb:window-inactive {
  background-color:#0F84FD;
}

.userNO{
  user-select: none;
}

body,
ul,
ol,
dl,
dd,
h1,
h2,
h3,
h4,
h5,
h6,
figure,
form,
fieldset,
legend,
input,
textarea,
button,
p,
blockquote,
th,
td,
pre,
xmp {
  margin: 0;
  padding: 0;
  font-family: tahoma, arial, "Hiragino Sans GB", simsun, sans-serif;
}

ul,
ol,
menu {
  list-style: none;
}

a,
ins,
s,
u,
del {
  border: none;
  text-decoration: none;
}

.fl {
  float: left;
}

.fr {
  float: right;
}

.ct {
  text-align: center;
}

.rt {
  text-align: right;
}

.clear {
  clear: both;
}

.ell{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

distance = 10 20
direction = t top, r right, b bottom, l left
for num in distance
  for dir in direction
    .m{dir[0]}{num}
      margin-{dir[1]}: 1px * num
    .p{dir[0]}{num}
      padding-{dir[1]}: 1px * num
